                                       <div class="invalid-feedback">Ingresar un dni válido</div>
                                 </div>
                              </div>

                              <div class="form-group row">
                                 <label for="tipoUsuario" class="col-md-4 col-sm-3 col-form-label label-gray-left">Tipo de Usuario:</label>
                                 <div class="col-md-7 col-sm-8">
                                    <select id="tipoUsuario" class="custom-select" name="tipoUsuario">
